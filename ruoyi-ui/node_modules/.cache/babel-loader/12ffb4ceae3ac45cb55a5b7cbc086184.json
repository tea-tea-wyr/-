;;AACA,cAAA,MAAA,CAAA,IAAA,GAAA,KAAA;;AACA,cAAA,MAAA,CAAA,OAAA;AACA,aAJA;AAKA,WANA,MAMA;AACA,6BAAA,MAAA,CAAA,IAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,cAAA,MAAA,CAAA,UAAA,CAAA,MAAA;;AACA,cAAA,MAAA,CAAA,IAAA,GAAA,KAAA;;AACA,cAAA,MAAA,CAAA,OAAA;AACA,aAJA;AAKA;AACA;AACA,OAhBA;AAiBA,KA7EA;;AA8EA;AACA,IAAA,YA/EA,wBA+EA,GA/EA,EA+EA;AAAA;;AACA,UAAA,IAAA,GAAA,GAAA,CAAA,GAAA,IAAA,KAAA,GAAA;AACA,WAAA,QAAA,CAAA,oBAAA,IAAA,GAAA,QAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,eAAA,iBAAA,IAAA,CAAA;AACA,OANA,EAMA,IANA,CAMA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA;;AACA,QAAA,MAAA,CAAA,UAAA,CAAA,MAAA;AACA,OATA;AAUA,KA3FA;;AA4FA;AACA,IAAA,YA7FA,0BA6FA;AAAA;;AACA,UAAA,WAAA,GAAA,KAAA,WAAA;AACA,WAAA,QAAA,CAAA,mBAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,eAAA,oBAAA,WAAA,CAAA;AACA,OANA,EAMA,IANA,CAMA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,QAAA,CAAA,GAAA;AACA,OARA;AASA;AAxGA;AAjDA,C","sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <el-form :model=\"queryParams\" ref=\"queryForm\" :inline=\"true\" v-show=\"showSearch\" label-width=\"68px\">\r\n<!--      <el-form-item label=\"密码\" prop=\"mpwd\">-->\r\n<!--        <el-input-->\r\n<!--          v-model=\"queryParams.mpwd\"-->\r\n<!--          placeholder=\"请输入密码\"-->\r\n<!--          clearable-->\r\n<!--          size=\"small\"-->\r\n<!--          @keyup.enter.native=\"handleQuery\"-->\r\n<!--        />-->\r\n<!--      </el-form-item>-->\r\n      <el-form-item label=\"昵称\" prop=\"mnick\">\r\n        <el-input\r\n          v-model=\"queryParams.mnick\"\r\n          placeholder=\"请输入昵称\"\r\n          clearable\r\n          size=\"small\"\r\n          @keyup.enter.native=\"handleQuery\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item label=\"角色\" prop=\"mrole\">\r\n        <el-input\r\n          v-model=\"queryParams.mrole\"\r\n          placeholder=\"请输入角色\"\r\n          clearable\r\n          size=\"small\"\r\n          @keyup.enter.native=\"handleQuery\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item label=\"权限\" prop=\"maccess\">\r\n        <el-input\r\n          v-model=\"queryParams.maccess\"\r\n          placeholder=\"请输入权限\"\r\n          clearable\r\n          size=\"small\"\r\n          @keyup.enter.native=\"handleQuery\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"cyan\" icon=\"el-icon-search\" size=\"mini\" @click=\"handleQuery\">搜索</el-button>\r\n        <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"resetQuery\">重置</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n\r\n    <el-row :gutter=\"10\" class=\"mb8\">\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"primary\"\r\n          icon=\"el-icon-plus\"\r\n          size=\"mini\"\r\n          @click=\"handleAdd\"\r\n          v-hasPermi=\"['system:adm:add']\"\r\n        >新增</el-button>\r\n      </el-col>\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"success\"\r\n          icon=\"el-icon-edit\"\r\n          size=\"mini\"\r\n          :disabled=\"single\"\r\n          @click=\"handleUpdate\"\r\n          v-hasPermi=\"['system:adm:edit']\"\r\n        >修改</el-button>\r\n      </el-col>\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"danger\"\r\n          icon=\"el-icon-delete\"\r\n          size=\"mini\"\r\n          :disabled=\"multiple\"\r\n          @click=\"handleDelete\"\r\n          v-hasPermi=\"['system:adm:remove']\"\r\n        >删除</el-button>\r\n      </el-col>\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"warning\"\r\n          icon=\"el-icon-download\"\r\n          size=\"mini\"\r\n          @click=\"handleExport\"\r\n          v-hasPermi=\"['system:adm:export']\"\r\n        >导出</el-button>\r\n      </el-col>\r\n      <right-toolbar :showSearch.sync=\"showSearch\" @queryTable=\"getList\"></right-toolbar>\r\n    </el-row>\r\n\r\n    <el-table v-loading=\"loading\" :data=\"admList\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\" align=\"center\" />\r\n      <el-table-column label=\"账号\" align=\"center\" prop=\"mno\" />\r\n      <el-table-column label=\"密码\" align=\"center\" prop=\"mpwd\" />\r\n      <el-table-column label=\"昵称\" align=\"center\" prop=\"mnick\" />\r\n      <el-table-column label=\"角色\" align=\"center\" prop=\"mrole\" />\r\n      <el-table-column label=\"权限\" align=\"center\" prop=\"maccess\" />\r\n      <el-table-column label=\"操作\" align=\"center\" class-name=\"small-padding fixed-width\">\r\n        <template slot-scope=\"scope\">\r\n\r\n          <el-row :gutter=\"10\" class=\"mb8\">\r\n          <el-col :span=\"1.5\">\r\n          <el-button\r\n            size=\"small\"\r\n            type=\"primary\"\r\n            icon=\"el-icon-edit\"\r\n            @click=\"handleUpdate(scope.row)\"\r\n            v-hasPermi=\"['system:adm:edit']\"\r\n          >修改</el-button>\r\n          </el-col>\r\n          <el-col :span=\"1.5\">\r\n          <el-button\r\n            size=\"small\"\r\n            type=\"danger\"\r\n            icon=\"el-icon-delete\"\r\n            @click=\"handleDelete(scope.row)\"\r\n            v-hasPermi=\"['system:adm:remove']\"\r\n          >删除</el-button>\r\n          </el-col>\r\n\r\n          </el-row>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <pagination\r\n      v-show=\"total>0\"\r\n      :total=\"total\"\r\n      :page.sync=\"queryParams.pageNum\"\r\n      :limit.sync=\"queryParams.pageSize\"\r\n      @pagination=\"getList\"\r\n    />\r\n\r\n    <!-- 添加或修改系统管理员对话框 -->\r\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"500px\" append-to-body>\r\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\r\n        <el-form-item label=\"密码\" prop=\"mpwd\">\r\n          <el-input v-model=\"form.mpwd\" placeholder=\"请输入密码\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"昵称\" prop=\"mnick\">\r\n          <el-input v-model=\"form.mnick\" placeholder=\"请输入昵称\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"角色\" prop=\"mrole\">\r\n          <el-input v-model=\"form.mrole\" placeholder=\"请输入角色（限一位数字）\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"权限\" prop=\"maccess\">\r\n          <el-input v-model=\"form.maccess\" placeholder=\"请输入权限（限一位数字）\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n        <el-button @click=\"cancel\">取 消</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { listAdm, getAdm, delAdm, addAdm, updateAdm, exportAdm } from \"@/api/system/adm\";\r\n\r\nexport default {\r\n  name: \"Adm\",\r\n  components: {\r\n  },\r\n  data() {\r\n    return {\r\n      // 遮罩层\r\n      loading: true,\r\n      // 选中数组\r\n      ids: [],\r\n      // 非单个禁用\r\n      single: true,\r\n      // 非多个禁用\r\n      multiple: true,\r\n      // 显示搜索条件\r\n      showSearch: true,\r\n      // 总条数\r\n      total: 0,\r\n      // 系统管理员表格数据\r\n      admList: [],\r\n      // 弹出层标题\r\n      title: \"\",\r\n      // 是否显示弹出层\r\n      open: false,\r\n      // 查询参数\r\n      queryParams: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        mpwd: null,\r\n        mnick: null,\r\n        mrole: null,\r\n        maccess: null\r\n      },\r\n      // 表单参数\r\n      form: {},\r\n      // 表单校验\r\n      rules: {\r\n        mpwd: [\r\n          { required: true, message: \"密码不能为空\", trigger: \"blur\" }\r\n        ],\r\n        mnick: [\r\n          { required: true, message: \"昵称不能为空\", trigger: \"blur\" }\r\n        ],\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.getList();\r\n  },\r\n  methods: {\r\n    /** 查询系统管理员列表 */\r\n    getList() {\r\n      this.loading = true;\r\n      listAdm(this.queryParams).then(response => {\r\n        this.admList = response.rows;\r\n        this.total = response.total;\r\n        this.loading = false;\r\n      });\r\n    },\r\n    // 取消按钮\r\n    cancel() {\r\n      this.open = false;\r\n      this.reset();\r\n    },\r\n    // 表单重置\r\n    reset() {\r\n      this.form = {\r\n        mno: null,\r\n        mpwd: null,\r\n        mnick: null,\r\n        mrole: null,\r\n        maccess: null\r\n      };\r\n      this.resetForm(\"form\");\r\n    },\r\n    // /** 搜索按钮操作 */\r\n    // handleQuery() {\r\n    //   this.queryParams.pageNum = 1;\r\n    //   this.getList();\r\n    // },\r\n    // /** 重置按钮操作 */\r\n    // resetQuery() {\r\n    //   this.resetForm(\"queryForm\");\r\n    //   this.handleQuery();\r\n    // },\r\n    // 多选框选中数据\r\n    handleSelectionChange(selection) {\r\n      this.ids = selection.map(item => item.mno)\r\n      this.single = selection.length!==1\r\n      this.multiple = !selection.length\r\n    },\r\n    /** 新增按钮操作 */\r\n    handleAdd() {\r\n      this.reset();\r\n      this.open = true;\r\n      this.title = \"添加系统管理员\";\r\n    },\r\n    /** 修改按钮操作 */\r\n    handleUpdate(row) {\r\n      this.reset();\r\n      const mno = row.mno || this.ids\r\n      getAdm(mno).then(response => {\r\n        this.form = response.data;\r\n        this.open = true;\r\n        this.title = \"修改系统管理员\";\r\n      });\r\n    },\r\n    /** 提交按钮 */\r\n    submitForm() {\r\n      this.$refs[\"form\"].validate(valid => {\r\n        if (valid) {\r\n          if (this.form.mno != null) {\r\n            updateAdm(this.form).then(response => {\r\n              this.msgSuccess(\"修改成功\");\r\n              this.open = false;\r\n              this.getList();\r\n            });\r\n          } else {\r\n            addAdm(this.form).then(response => {\r\n              this.msgSuccess(\"新增成功\");\r\n              this.open = false;\r\n              this.getList();\r\n            });\r\n          }\r\n        }\r\n      });\r\n    },\r\n    /** 删除按钮操作 */\r\n    handleDelete(row) {\r\n      const mnos = row.mno || this.ids;\r\n      this.$confirm('是否确认删除系统管理员编号为\"' + mnos + '\"的数据项?', \"警告\", {\r\n          confirmButtonText: \"确定\",\r\n          cancelButtonText: \"取消\",\r\n          type: \"warning\"\r\n        }).then(function() {\r\n          return delAdm(mnos);\r\n        }).then(() => {\r\n          this.getList();\r\n          this.msgSuccess(\"删除成功\");\r\n        })\r\n    },\r\n    /** 导出按钮操作 */\r\n    handleExport() {\r\n      const queryParams = this.queryParams;\r\n      this.$confirm('是否确认导出所有系统管理员数据项?', \"警告\", {\r\n          confirmButtonText: \"确定\",\r\n          cancelButtonText: \"取消\",\r\n          type: \"warning\"\r\n        }).then(function() {\r\n          return exportAdm(queryParams);\r\n        }).then(response => {\r\n          this.download(response.msg);\r\n        })\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/system/adm"}]}